{
    "manifest_version": 2,
    "name": "DataEntryExtension",
    "description": "Chrome extension that is used for Quicker data entry",
    "version": "1.0.0",
    "icons": {
        "16": "logo192.png",
        "48": "logo192.png",
        "128": "logo512.png"
    },
    "browser_action": {
        "default_icon": "favicon.ico",
        "default_title": "Quicker Data Entry Extension"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "all_frames": true,
            "js": ["./content.js", "./jquery.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": ["activeTab"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": ["index.html", "/static/*"]
}
